             xmlns:local="clr-namespace:SodiumPaint"
            >
    <Application.Resources>
       

        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>

                <ResourceDictionary Source="Resources/Colors.xaml"/>
